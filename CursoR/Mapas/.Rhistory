Matricula <- 246
# Gerando os dados
n1 <- 10; m1 <- 290; s1 <- 12; n2 <- 16; m2 <- 321; s2 <- 22
set.seed(Matricula)
fornecedor1 <- round(rnorm(n1, mean=m1, sd=s1),0)
set.seed(Matricula)
fornecedor2 <- round(rnorm(n2, mean=m2, sd=s2),0)
# Exibindo os dados
fornecedor1
fornecedor2
boxplot(fornecedor1,fornecedor2,names=c(1,2),xlab="Fornecedor",ylab="Resistência (lbf-ft)")
c(xbar=mean(fornecedor1),dp=sd(fornecedor1))
c(xbar=mean(fornecedor2),dp=sd(fornecedor2))
var.test(fornecedor1,fornecedor2)
t.test(fornecedor1,fornecedor2,var.equal=FALSE)
